<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools" android:layout_width="match_parent"
    android:layout_height="match_parent" android:paddingLeft="@dimen/activity_horizontal_margin"
    android:paddingRight="@dimen/activity_horizontal_margin"
    android:paddingTop="@dimen/activity_vertical_margin"
    android:paddingBottom="@dimen/activity_vertical_margin"
    tools:context="br.com.vempracaruaru.activitys.HomeActivity"
    android:background="@drawable/fundo">


    <ImageView
        android:id="@+id/logo"
        android:layout_width="200dp"
        android:layout_height="200dp"
        android:background="@drawable/logo"
        android:layout_centerHorizontal="true" />

    <ImageButton
        android:layout_width="wrap_content"
        android:layout_height="60dp"
        android:id="@+id/ibt_pontos_turisticos"
        android:layout_below="@id/logo"
        android:background="@drawable/btn_pontos_turisticos" />


    <ImageButton
        android:layout_width="wrap_content"
        android:layout_height="60dp"
        android:id="@+id/ibt_artistas"
        android:layout_below="@id/ibt_pontos_turisticos"
        android:background="@drawable/btn_artistas" />


    <ImageButton
        android:layout_width="wrap_content"
        android:layout_height="60dp"
        android:id="@+id/ibt_obras"
        android:layout_below="@id/ibt_artistas"
        android:background="@drawable/btn_obras" />


    <ImageButton
        android:layout_width="wrap_content"
        android:layout_height="60dp"
        android:id="@+id/ibt_minha_conta"
        android:layout_below="@id/ibt_obras"
        android:background="@drawable/btn_minha_conta" />

</RelativeLayout>
